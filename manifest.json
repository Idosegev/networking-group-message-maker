{
  "name": "Satzah Networking Message Maker",
  "description": "Build a message for our networking community!",
  "version": "1.1",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon-128.png"
  },
  "icons": {
    "128":"icon-128.png"
  }
  
}
